{
    "minimum-stability": "dev",
    "name": "flight-base-app",
    "version": "0.1.0",
    "config": {
        "vendor-dir": "libraries"
    },

    "require": {
        "mikecao/flight": "dev-master",
        "erusev/parsedown": "dev-master"
    },

    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "phpunit/phpunit-selenium": ">=1.2",
        "phing/phing": "2.*",
        "squizlabs/php_codesniffer": "1.*"
    },

    "autoload": {
        "psr-4": {
            "fmvc\\core\\": "core",
            "fmvc\\application\\": "application"
        }
    }
}